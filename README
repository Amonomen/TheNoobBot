To be used to replace KeyPress things. (buggy on Mouse buttons, etc...)
public void Ascend()
{
	Lua.ExecuteBuffer(JumpOrAscendStart());
}

public void Jump()
{
	Lua.ExecuteBuffer(JumpOrAscendStart());
}

public void Descend()
{
	Lua.ExecuteBuffer(SitStandOrDescendStart());
}

public void MoveBackward()
{
	Lua.ExecuteBuffer(MoveBackwardStart());
}

public void MoveForward()
{
	Lua.ExecuteBuffer(MoveForwardStart());
}


public void StrafeLeft()
{
	Lua.ExecuteBuffer(StrafeLeftStart());
}

public void StrafeRight()
{
	Lua.ExecuteBuffer(StrafeRightStart());
}